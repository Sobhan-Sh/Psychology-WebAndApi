@{
    ViewData["Title"] = "اضافه شدن به کادر درمان_بخش اخر";
}
<section class="container row pt-4 mt-5 pt-md-2 mt-md-3 mx-auto px-0 px-md-2">
    <section class="col-11 col-md-9 mx-auto mx-md-0 mt-3 mt-md-0 px-0 px-md-2">
        <!--* card -->
        <section class="card mx-auto pt-5 pb-3 card-profile-form">
            <!--* body card -->
            <section class="card-body px-3">
                <div class="custom_alert mt-2 border-2 border-info custom_alert_primary mx-auto">
                    <div class="icon__wrapper" onclick="document.querySelector('.custom_alert').remove()">
                        <span class="mdi mdi-alert-outline"></span>
                    </div>
                    <p class="mb-0"> شما با موفقیت به کادر اضافه شدید و پس از تایید ادمین می توانید کار کنید </p>
                </div>
            </section>
        </section>
    </section>
</section>